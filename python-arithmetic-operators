https://www.hackerrank.com/challenges/python-arithmetic-operators/problem

def add (a, b):
    c = a + b
    print (c)

def sub (a, b):
    c = a - b
    print (c)

def prod(a, b):
    c = a * b
    print (c)

if __name__ == '__main__':
    a = int(raw_input())
    b = int(raw_input())
    add (a, b)
    sub (a, b)
    prod (a, b)
