https://www.hackerrank.com/challenges/find-second-maximum-number-in-a-list/problem

if __name__ == '__main__':
    n = int(raw_input())
    arr = map(int, raw_input().split())
    large = max(arr)
    i = 0
    for i in range(n):
        if large == max(arr):
            arr.remove(max(arr))
 
    print (max(arr)) 
